#===============================================================================

# local variable
#===============================================================================

APP = nand_product
OBJS = nandc_ecc.o oob_config_v100.o main.o nandc_random.o
#===============================================================================
# rules
#===============================================================================
.PHONY: all clean install uninstall

all:$(APP)

%.o: %.c
	$(AT)$(CC)  -c -o $@ $^

$(APP): ${OBJS}
	$(AT)$(CC) -static -o $@ $^

clean:
	-$(AT)rm -f ${OBJS} ${APP}

install:

uninstall:
